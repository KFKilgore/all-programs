<h1>Pages#vehicles</h1>
<p>A Vehicle resource is a single transport craft that does not have hyperdrive capability.</p>

<table>
    <tbody>
        <tr>
            <th>name</th>
            <th>model</th>
            <th>class</th>
            <th>cost</th>
            <th>speed</th>
            <th>crew</th>
            <th>passengers</th>
            <th>consumables</th>
            <!--
            <th>films</th>
            <th>pilots</th>
            -->
        </tr>
        
        <% @results.each do |vehicle| %>
        
        <tr>
            <td> <%= vehicle["name"] %> </td>
            <td> <%= vehicle["model"] %> </td>
            <td> <%= vehicle["vehicle_class"] %> </td>
            <td> <%= vehicle["cost_in_credits"] %> </td>
            <td> <%= vehicle["max_atmosphering_speed"] %> </td>
            <td> <%= vehicle["crew"] %> </td>
            <td> <%= vehicle["passengers"] %> </td>
            <td> <%= vehicle["consumables"] %> </td>
            <!--
            <td> <%= vehicle["films"] %> </td>
            <td> <%= vehicle["pilots"] %> </td>
            -->
        </tr>
        
        <% end %>
        
    </tbody>
</table>


