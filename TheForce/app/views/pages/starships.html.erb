<h1>Starships</h1>
<p>A Starship resource is a single transport craft that has hyperdrive capability.</p>

<table>
    <tbody>
        <tr>
            <th>name</th>
            <th>model</th>
            <th>class</th>
            <th>cost</th>
            <th>hyperdrive rating</th>
            <th>speed</th>
            <th>megalights/hr</th>
            <th>consumables</th>
           <!-- <th>films</th>
            <th>pilots</th> -->
        </tr>
        
        <% @results.each do |starship| %>
        
        <tr>
            <td> <%= starship["name"] %> </td>
            <td> <%= starship["model"] %> </td>
            <td> <%= starship["starship_class"] %> </td>
            <td> <%= starship["cost_in_credits"] %> </td>
            <td> <%= starship["hyperdrive_rating"] %> </td>
            <td> <%= starship["max_atmosphering_speed"] %> </td>
            <td> <%= starship["MGLT"] %> </td>
            <td> <%= starship["consumables"] %> </td>
        <!--    <td> <%= starship["films"] %> </td>
            <td> <%= starship["pilots"] %> </td>  -->
        </tr>
        
        <% end %>
        
    </tbody>
</table>


